      ]
    ],
    [
      1,
      "p",
      [
        [
          0,
          [],
          0,
          "The next year, Warren "
        ],
        [
          0,
          [
            20
          ],
          1,
          "opposed"
        ],
        [
          0,
          [],
          0,
