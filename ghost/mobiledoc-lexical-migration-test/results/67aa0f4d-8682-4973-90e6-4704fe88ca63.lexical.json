            "version": 1
          },
          {
            "type": "linebreak",
            "version": 1
          },
          {
            "detail": 0,
            "format": 3,
            "mode": "normal",
            "style": "",
            "text": "More classic cars:",
            "type": "text",
            "version": 1
          },
