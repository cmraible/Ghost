        "metadata": {
          "title": "HYPERCHARGE: Unboxed - First-person shooter. Nintendo Switch: Official Launch Trailer",
          "author_name": "Digital Cybercherries",
          "author_url": "https://www.youtube.com/c/Digitalcybercherries",
          "height": 113,
          "width": 200,
          "version": "1.0",
          "provider_name": "YouTube",
          "provider_url": "https://www.youtube.com/",
