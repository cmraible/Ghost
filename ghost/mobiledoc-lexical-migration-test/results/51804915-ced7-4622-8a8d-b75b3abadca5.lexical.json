            "style": "",
            "text": " to create this custom cyberpunk ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "BMW",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
