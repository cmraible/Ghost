          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "It was all done for a good cause…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
        "tag": "h3",
        "version": 1
