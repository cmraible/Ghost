        "html": "<iframe width=\"612\" height=\"344\" src=\"https://www.youtube.com/embed/MYicXTr5M0Y?feature=oembed\" frameborder=\"0\" allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>",
        "metadata": {
          "thumbnail_height": 360,
          "provider_name": "YouTube",
          "title": "1967 Ford Mustang Fastback Custom Ringbrothers Copperback Test Drive",
          "height": 344,
          "provider_url": "https://www.youtube.com/",
          "thumbnail_width": 480,
          "author_url": "https://www.youtube.com/channel/UCBLjaLyt0Zb79MRJzRFJBdQ",
