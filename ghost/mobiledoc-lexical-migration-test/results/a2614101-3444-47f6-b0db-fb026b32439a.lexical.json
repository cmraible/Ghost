          {
            "detail": 0,
            "format": 1,
            "mode": "normal",
            "style": "",
            "text": "Check out the Hertz stolen car settlement ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
