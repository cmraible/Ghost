          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " (The Intercept)  — ",
            "type": "text",
            "version": 1
          },
          {
            "detail": 0,
            "format": 2,
            "mode": "normal",
            "style": "",
            "text": "Regular ",
