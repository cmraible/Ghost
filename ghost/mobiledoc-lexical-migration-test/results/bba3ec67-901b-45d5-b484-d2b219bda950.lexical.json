          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "It’s been a rough year for the ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Chevrolet Camaro",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
