          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ", showing that what was once a despised joke in the history of Ford has now become sought after.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
