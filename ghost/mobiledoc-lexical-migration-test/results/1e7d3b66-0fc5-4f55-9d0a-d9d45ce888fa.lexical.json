            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "don’t fret",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/news/demon-170-markups/",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ". There’s a solution to enjoying the same kind of power, if you already have or can get your hands on a newer Challenger Hellcat.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
