          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "6. Forever hunting business models",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
