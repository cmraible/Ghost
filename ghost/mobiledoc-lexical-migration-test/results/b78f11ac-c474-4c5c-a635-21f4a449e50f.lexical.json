              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
                "style": "",
                "text": "1969 Buick Riviera Puts Fine American Styling On Display",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/highlights/1969-buick-riviera-puts-fine-american-styling-on-display",
