          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "That toothy grin looks a little battered…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
