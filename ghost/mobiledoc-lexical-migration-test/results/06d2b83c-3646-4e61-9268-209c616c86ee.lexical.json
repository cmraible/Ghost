            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " twice a week for decades.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
