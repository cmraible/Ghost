        "children": [
          {
            "detail": 0,
            "format": 1,
            "mode": "normal",
            "style": "",
            "text": "Check out a car collection flooded by Hurricane Ian ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
