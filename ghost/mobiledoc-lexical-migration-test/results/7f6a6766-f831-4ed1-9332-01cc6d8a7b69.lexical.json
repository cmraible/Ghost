          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Don’t you want to own nothing by 2030?",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
