          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "A lot of people love the modern ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Shelby GT350R",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
