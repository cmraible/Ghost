                  {
                    "detail": 0,
                    "format": 0,
                    "mode": "normal",
                    "style": "",
                    "text": "Driving A Homemade Ford Escort RS Cosworth World Rally Car",
                    "type": "text",
                    "version": 1
                  }
                ],
                "direction": "ltr",
