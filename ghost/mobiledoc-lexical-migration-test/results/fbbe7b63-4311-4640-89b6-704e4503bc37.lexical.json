                "children": [
                  {
                    "detail": 0,
                    "format": 0,
                    "mode": "normal",
                    "style": "",
                    "text": "Citroen's Forgotten BX 4TC Group B Car Is A Potential Bargain",
                    "type": "text",
                    "version": 1
                  }
                ],
                "direction": "ltr",
