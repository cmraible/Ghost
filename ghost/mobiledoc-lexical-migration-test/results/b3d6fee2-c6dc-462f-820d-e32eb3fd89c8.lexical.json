            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "GTO",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/features-3/five-quick-facts-about-the-pontiac-gto/",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " and a 1953 Buick Skylark.",
            "type": "text",
            "version": 1
