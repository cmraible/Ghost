          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Corvettes typically look like ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "other Corvettes",
