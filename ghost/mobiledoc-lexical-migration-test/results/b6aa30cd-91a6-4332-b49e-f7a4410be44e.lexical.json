        "version": 1
      },
      {
        "children": [
          {
            "detail": 0,
            "format": 2,
            "mode": "normal",
            "style": "",
            "text": "See a Dodge Challenger Hellcat Redeye hearse ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 2,
                "mode": "normal",
                "style": "",
                "text": "here",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
