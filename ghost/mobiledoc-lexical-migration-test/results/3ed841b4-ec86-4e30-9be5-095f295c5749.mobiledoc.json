    [
      10,
      1
    ],
    [
      1,
      "p",
      [
        [
          0,
          [],
          0,
          "People understandably love "
        ],
        [
          0,
          [
            5
          ],
          1,
          "Panteras"
        ],
        [
          0,
          [],
          0,
          " because they’re crazy in all the right ways. Stuffing a big "
        ],
        [
          0,
          [
            6
          ],
          1,
          "American V8"
