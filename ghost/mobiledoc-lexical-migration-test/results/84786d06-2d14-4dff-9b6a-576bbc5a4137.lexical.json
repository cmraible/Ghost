          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " are great but we’re fans of variety.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
      },
