          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "The court ",
                "type": "text",
                "version": 1
              },
              {
                "children": [
                  {
                    "detail": 0,
                    "format": 0,
                    "mode": "normal",
                    "style": "",
                    "text": "limited",
                    "type": "text",
                    "version": 1
                  }
                ],
