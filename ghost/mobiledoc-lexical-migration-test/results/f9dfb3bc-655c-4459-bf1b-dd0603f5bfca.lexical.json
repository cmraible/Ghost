          {
            "detail": 0,
            "format": 1,
            "mode": "normal",
            "style": "",
            "text": "This reminds us of another bedazzled vehicle ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
