          [
            2
          ],
          1,
          "Charged Garage Instagram"
        ],
        [
          0,
          [],
          0,
          " page to see this Ford Cortina's documented transformation to an electric vehicle from the first steps back in 2018 to the final product."
        ]
      ]
    ],
