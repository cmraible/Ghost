          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " were the one he was riding in Dallas when he was assassinated.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
      },
      {
        "type": "image",
        "src": "__GHOST_URL__/content/images/2020/09/kennedy2.jpeg",
        "width": 1049,
