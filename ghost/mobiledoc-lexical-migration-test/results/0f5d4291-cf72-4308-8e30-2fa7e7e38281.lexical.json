          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "The man has good taste‚Ä¶",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
