          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "The 1977 Corvette did receive minor changes, but they were more about branding than styling or performance.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
        "tag": "h3",
        "version": 1
