          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Is this the new Apple iPhone?",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
