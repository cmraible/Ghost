          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ". “For the younger generation, those rules don’t apply at all.”",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
