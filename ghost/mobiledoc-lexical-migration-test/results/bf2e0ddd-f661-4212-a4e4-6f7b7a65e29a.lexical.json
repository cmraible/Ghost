              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Tyler Hoover",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
