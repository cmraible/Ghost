              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": ", editorial director, Kent Messenger Media Group",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "listitem",
