            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/news/youtuber-c8-corvette-wheels/",
            "version": 1
