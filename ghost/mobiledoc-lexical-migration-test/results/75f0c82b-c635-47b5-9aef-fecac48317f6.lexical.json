              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
                "style": "",
                "text": "Is Senna’s NSX Type-R Legacy The Finest JDM Car Ever Made?",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
