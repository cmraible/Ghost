          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ", and ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "interest is waning",
