              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
                "style": "",
                "text": "Google Cloud Next’20",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
