          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Sadly, 2020 has been the year of ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
