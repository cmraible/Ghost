          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Wait until you see what’s in the pot at the end…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
