            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": "noopener",
            "target": null,
            "title": null,
            "url": "https://www.girardo.com/available/1985-lancia-delta-s4-corsa-group-b_1/?utm_campaign=motorious&utm_medium=referral&utm_source=motorious.com",
            "version": 1
          }
