          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "and Gemballa  in Germany, ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Singer",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
