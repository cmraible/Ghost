        "html": "<iframe width=\"612\" height=\"344\" src=\"https://www.youtube.com/embed/bkI-iDhyOHY?feature=oembed\" frameborder=\"0\" allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>",
        "metadata": {
          "author_url": "https://www.youtube.com/user/RMAuctionsInc",
          "height": 344,
          "version": "1.0",
          "thumbnail_height": 360,
          "provider_url": "https://www.youtube.com/",
          "provider_name": "YouTube",
          "width": 612,
          "thumbnail_width": 480,
          "thumbnail_url": "https://i.ytimg.com/vi/bkI-iDhyOHY/hqdefault.jpg",
          "author_name": "RM Sotheby's",
          "title": "Online Only: SHIFT/MONTEREY — 1972 Porsche 916"
        },
        "embedType": "video"
