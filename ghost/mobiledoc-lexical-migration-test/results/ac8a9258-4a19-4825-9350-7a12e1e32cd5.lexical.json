      {
        "type": "embed",
        "url": "https://www.youtube.com/watch?v=04bhuNfgxu0",
        "html": "<iframe width=\"612\" height=\"344\" src=\"https://www.youtube.com/embed/04bhuNfgxu0?feature=oembed\" frameborder=\"0\" allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>",
        "metadata": {
          "thumbnail_url": "https://i.ytimg.com/vi/04bhuNfgxu0/hqdefault.jpg",
          "title": "Man who paid $75 for Chevy truck 44 years ago, just sold it to prior owner's grandson for $75",
          "author_url": "https://www.youtube.com/user/KARE11",
          "thumbnail_width": 480,
          "version": "1.0",
          "width": 612,
          "author_name": "KARE 11",
