          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Sadly, it’s doing better than other electric truck startups…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
