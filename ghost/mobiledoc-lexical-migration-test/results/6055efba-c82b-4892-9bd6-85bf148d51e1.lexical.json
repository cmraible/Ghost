          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "This is heartbreaking…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
