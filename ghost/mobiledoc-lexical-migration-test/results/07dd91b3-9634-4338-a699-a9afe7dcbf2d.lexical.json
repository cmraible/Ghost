            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/features-3/banned-1969-dodge-daytona/",
            "version": 1
