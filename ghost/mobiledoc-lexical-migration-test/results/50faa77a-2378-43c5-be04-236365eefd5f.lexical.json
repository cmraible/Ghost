              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "addictive to flog hard",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
