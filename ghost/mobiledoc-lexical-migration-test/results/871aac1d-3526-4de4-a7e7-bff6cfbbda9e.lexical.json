            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "- Nissan's Iconic RB26 Engine Returns To Production",
                "type": "text",
                "version": 1
              }
