          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Can you handle the electrified power?",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
