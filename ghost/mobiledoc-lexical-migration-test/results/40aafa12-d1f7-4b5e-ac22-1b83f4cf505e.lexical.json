          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "The rare 1954 Jaguar XK120 FHC in barn-find condition didn't reach near the projected sell through price at auction.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
