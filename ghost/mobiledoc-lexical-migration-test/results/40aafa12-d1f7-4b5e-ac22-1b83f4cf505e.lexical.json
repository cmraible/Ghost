            "url": "https://themarket.co.uk/listings/jaguar/xk120-fhc/763a84e9-bcd9-4295-9752-6dd2bec903f6?status=preview",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " in ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "barn find",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
