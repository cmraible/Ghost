          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " has continued, meaning the use of a leprechaun probably indicates some strong tie with the yet-to-be-revealed car.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
