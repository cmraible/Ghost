          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "1970 marked the second-generation of the Firebird. ",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
