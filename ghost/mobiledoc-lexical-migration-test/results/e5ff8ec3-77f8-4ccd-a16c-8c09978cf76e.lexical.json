            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/handpicked/rarest-porsches-ever/",
            "version": 1
