                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Ford Mustang Foxbody",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
