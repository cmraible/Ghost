          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "People have been fantasizing about shoving the ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Hellcat",
                "type": "text",
                "version": 1
              }
