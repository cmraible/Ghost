          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "This off-roader is a lot of fun all around.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
        "tag": "h3",
        "version": 1
