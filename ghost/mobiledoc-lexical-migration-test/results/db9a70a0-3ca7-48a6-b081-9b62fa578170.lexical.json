          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Now everyone can see it up close…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
