          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "The memory of this Spanish motorcycle has waned over the years.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
