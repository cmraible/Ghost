        "metadata": {
          "author_name": "Art & Speed Classic Cars",
          "height": 344,
          "author_url": "https://www.youtube.com/channel/UCLPDUXugV1e1JrYNDnytJzQ",
          "thumbnail_url": "https://i.ytimg.com/vi/N_eA8RWuk6s/hqdefault.jpg",
          "thumbnail_width": 480,
          "provider_name": "YouTube",
          "title": "1932 Ford 5 Window. www.Artandspeed.com",
