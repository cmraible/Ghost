          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "This is pretty crazy…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
