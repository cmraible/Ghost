          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "While we certainly love the ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "rumble of an American V8",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
