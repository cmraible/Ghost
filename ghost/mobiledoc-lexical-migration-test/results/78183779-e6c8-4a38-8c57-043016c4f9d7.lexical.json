          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "GTX",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/news/1968-plymouth-gtx/",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " instead of a bare bones ",
            "type": "text",
            "version": 1
