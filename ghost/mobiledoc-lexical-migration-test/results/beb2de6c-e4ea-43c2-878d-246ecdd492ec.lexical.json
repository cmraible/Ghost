          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ", we have a feeling that other tuners will get in on the 200-mph action!",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
