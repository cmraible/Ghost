          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Only 54 W30s were built by the factory, although an additional 97 were produced for dealer installation.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
        "tag": "h3",
