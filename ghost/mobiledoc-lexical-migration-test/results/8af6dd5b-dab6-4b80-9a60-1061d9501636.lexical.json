          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Refuel where so many Americans gathered to socialize on the Mother Road.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
