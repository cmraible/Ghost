          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Corvette speed made her day!",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
        "tag": "h2",
        "version": 1
