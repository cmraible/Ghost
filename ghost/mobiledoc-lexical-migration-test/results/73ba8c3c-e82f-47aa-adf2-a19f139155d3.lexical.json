          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " is pleased to announce this one-owner ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "1983 Porsche 944",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
