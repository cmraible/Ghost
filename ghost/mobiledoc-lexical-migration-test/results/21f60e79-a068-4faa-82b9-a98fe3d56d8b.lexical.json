          {
            "detail": 0,
            "format": 2,
            "mode": "normal",
            "style": "",
            "text": "Unto the End",
            "type": "text",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ", immerse yourself in a handcrafted adventure about a father doing everything in his power to return home to his family; and when all else fails, just remember, read-react-survive.",
            "type": "text",
            "version": 1
          }
        ],
