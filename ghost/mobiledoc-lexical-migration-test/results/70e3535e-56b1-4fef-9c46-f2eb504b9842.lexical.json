        "type": "horizontalrule"
      },
      {
        "children": [
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "This could possibly be the world’s only front-wheel-drive ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "Corvette",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
