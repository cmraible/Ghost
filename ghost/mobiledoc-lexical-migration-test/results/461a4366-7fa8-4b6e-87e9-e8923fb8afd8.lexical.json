          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " has this incredible, low-mileage Supra listed for just $13,990!",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
