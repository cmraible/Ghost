          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " could be right up your alley.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
      },
      {
        "type": "image",
        "src": "__GHOST_URL__/content/images/2020/10/Motorious---3--14.jpg",
        "width": 1920,
