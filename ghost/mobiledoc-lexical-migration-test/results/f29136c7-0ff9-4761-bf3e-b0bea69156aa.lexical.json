          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Yet another event has been claimed by the COVID-19 pandemic.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
