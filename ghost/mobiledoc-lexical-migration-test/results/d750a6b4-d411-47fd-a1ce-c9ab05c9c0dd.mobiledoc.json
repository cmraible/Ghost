        [
          0,
          [
            20
          ],
          1,
          "passed the codes"
        ],
        [
          0,
          [],
          0,
          "."
        ]
      ]
    ],
    [
      10,
      4
    ],
    [
      1,
      "p",
      [
        [
          0,
          [],
          0,
          "“The family code is the result of a collective endeavor,” Yamila Gonzalez of the Families Code Drafting Committee "
        ],
        [
          0,
          [
            21
          ],
          1,
          "told"
        ],
        [
          0,
          [],
