      {
        "type": "email",
        "html": "<p><em><strong><a href=\"https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Fwww.levernews.com%2Fbiden-historic-tax-cut%2F&amp;amp;src=sdkpreparse\">Click to share on Facebook</a></strong></em></p>"
      },
      {
        "children": [
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Unite the Country, a super PAC that supports President Joe Biden, ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "has been",
                "type": "text",
                "version": 1
              }
            ],
