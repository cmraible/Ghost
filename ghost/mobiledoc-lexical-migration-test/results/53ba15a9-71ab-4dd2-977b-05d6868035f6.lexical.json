          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "It might be worth hunting one of these down.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
        "tag": "h2",
