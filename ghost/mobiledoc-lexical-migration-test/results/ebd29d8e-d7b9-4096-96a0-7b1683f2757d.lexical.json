            "children": [
              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
                "style": "",
                "text": "50 Years On: Ford's 10 Greatest Ever Capris",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
