              {
                "detail": 0,
                "format": 1,
                "mode": "normal",
                "style": "",
                "text": "Is This The Perfect R32 Nissan Skyline GT-R?",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
