          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Business up front. Party out back.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "koenig-heading",
        "tag": "h2",
