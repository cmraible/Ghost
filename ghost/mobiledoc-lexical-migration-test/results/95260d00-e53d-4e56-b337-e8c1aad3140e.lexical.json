          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " comes from the fact that there are less than 13,000 original miles showing on the odometer.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "paragraph",
        "version": 1
