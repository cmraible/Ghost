          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " marked the last of the first generations and is arguably the most popular.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
        "tag": "h3",
        "version": 1
      },
      {
        "type": "image",
