              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "More Information",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
            "rel": null,
            "target": null,
            "title": null,
            "url": "https://buy.motorious.com/vehicles/183646/1993-toyota-supra-rz",
            "version": 1
