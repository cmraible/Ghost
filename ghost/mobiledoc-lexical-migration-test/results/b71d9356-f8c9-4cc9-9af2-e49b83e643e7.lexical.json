            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "However, well-written sub-headings, which are informative to the reader, will also help Google understand the content better, and thus rank it for appropriate keywords.",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "listitem",
