            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ", making it not only completely authentic but also a true collectible.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
