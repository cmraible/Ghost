        "indent": 0,
        "type": "paragraph",
        "version": 1
      },
      {
        "type": "embed",
        "url": "https://www.youtube.com/watch?v=rhN0wQZhmjo",
        "html": "<iframe width=\"612\" height=\"344\" src=\"https://www.youtube.com/embed/rhN0wQZhmjo?feature=oembed\" frameborder=\"0\" allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>",
        "metadata": {
          "version": "1.0",
          "thumbnail_url": "https://i.ytimg.com/vi/rhN0wQZhmjo/hqdefault.jpg",
          "provider_name": "YouTube",
