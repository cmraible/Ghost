              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "One-of-a-kind cars",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
            "format": "",
            "indent": 0,
            "type": "link",
