          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "On ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "three separate occasions",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
