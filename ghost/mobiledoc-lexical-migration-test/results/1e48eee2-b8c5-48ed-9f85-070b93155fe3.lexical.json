          {
            "detail": 0,
            "format": 1,
            "mode": "normal",
            "style": "",
            "text": "Amy",
            "type": "text",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": " thinks that membership growth is dependent on getting close to the readers, and working out what they want, not just what they say they want.",
            "type": "text",
            "version": 1
