          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Brought back from near extinction, the Corvette returned better than ever in 1956.",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
