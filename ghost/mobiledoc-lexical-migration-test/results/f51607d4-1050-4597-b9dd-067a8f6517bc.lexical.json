        "children": [
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "A lot of people apparently expect the most ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "stolen vehicle",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
