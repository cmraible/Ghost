            "rel": null,
            "target": null,
            "title": null,
            "url": "__GHOST_URL__/articles/news/social-media-influencers/",
            "version": 1
          },
          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": ". Well, ",
            "type": "text",
            "version": 1
          },
          {
            "children": [
              {
                "detail": 0,
                "format": 0,
                "mode": "normal",
                "style": "",
                "text": "the inevitable has happened",
                "type": "text",
                "version": 1
              }
            ],
            "direction": "ltr",
