          {
            "detail": 0,
            "format": 0,
            "mode": "normal",
            "style": "",
            "text": "Looks pretty cool, extra power not included…",
            "type": "text",
            "version": 1
          }
        ],
        "direction": "ltr",
        "format": "",
        "indent": 0,
        "type": "heading",
